{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*",
  "management": {
    "endpoints": {
      "path": "/",
      "actuator": {
        "exposure": {
          "include": ["env", "refresh", "prometheus"]
        },
        "health": {
          "showdetails": "always"
        }
      }
    }
  },
  "consul": {
    "host": "consul.service",
    "discovery": {
      "scheme": "http",
      "port": 80,
      "healthCheckPath": "/health",
      "serviceName": "apiinsghts",
      "tags": ["api", "rest", "aspnetcore"],
      "heartbeat": {
        "enabled": false
      },
      "register": true,
      "deregister": true
    }
  }
}
